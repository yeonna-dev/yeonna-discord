{
  "name": "yeonna-discord",
  "version": "0.0.1",
  "description": "Discord Bot client of the Yeonna project",
  "main": "dist/discord.js",
  "scripts": {
    "start": "tsc && node dist/start.js",
    "start-dev": "ts-node --transpile-only src/start.ts",
    "dev": "nodemon --watch 'src/**' --ext 'ts,json' --exec 'npm run start-dev'",
    "update-core": "pnpm rm yeonna-core && pnpm i yeonna-dev/yeonna-core",
    "update-config": "pnpm rm yeonna-config && pnpm i yeonna-dev/yeonna-config",
    "update-comtroller": "pnpm rm comtroller && pnpm i esfox/comtroller"
  },
  "author": "osairis100@gmail.com",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.4",
    "cheerio": "^1.0.0-rc.10",
    "comtroller": "github:esfox/comtroller",
    "discord.js": "^13.3.0",
    "jsonfile": "^6.1.0",
    "node-schedule": "^2.0.0",
    "sharp": "^0.29.3",
    "table": "^6.7.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "yeonna-config": "github:yeonna-dev/yeonna-config",
    "yeonna-core": "github:yeonna-dev/yeonna-core"
  },
  "devDependencies": {
    "@types/jsonfile": "^6.0.1",
    "@types/node": "^15.14.9",
    "@types/node-schedule": "^1.3.2",
    "@types/sharp": "^0.29.5",
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}